<main class="_home _1">
  <section class="_i">
    <div class="_container _1">
      <div class="_cardhome">
        <div class="row no-m">
          <div class="col-12 col-md-6 col-xl-5 _1">
            <div class="_inside">
              <div class="_info">
                <div class="_content" data-splitting>think outside the (stock) box</div>
              </div>
              <figure class="_food pollo">
                <img src="https://cdn.shopify.com/s/files/1/0901/1216/4158/files/chicken.svg?v=1736975863" />
              </figure>
              <figure class="_food cebolla">
                <img src="https://cdn.shopify.com/s/files/1/0901/1216/4158/files/cebolla.svg?v=1736975862" />
              </figure>
              <figure class="_food lemon">
                <img src="https://cdn.shopify.com/s/files/1/0901/1216/4158/files/pepino.svg?v=1736975862" />
              </figure>
              <figure class="_food cilantro">
                <img src="https://cdn.shopify.com/s/files/1/0901/1216/4158/files/cilantro.svg?v=1736975862" />
              </figure>
            </div>
          </div>
          <div class="col-12 col-md-6 col-xl-7 _2">
            <div class="_inside">
              <figure class="_video">
                <video class="_video-in" src="https://cdn.shopify.com/videos/c/o/v/e26f00b1c5814660bc9ae194013bb715.mp4" autoplay muted loop playsinline></video>
              </figure>
              <div class="_logic">
                <div class="_item _1">
                  <img src="https://cdn.shopify.com/s/files/1/0901/1216/4158/files/stick.svg?v=1736976069">
                  <h5>open stick</h5>
                </div>
                <div class="_icon _1">
                  <img src="https://cdn.shopify.com/s/files/1/0901/1216/4158/files/plus.svg?v=1736976069">
                </div>
                <div class="_item _2">
                  <img src="https://cdn.shopify.com/s/files/1/0901/1216/4158/files/hotwater.svg?v=1736976069">
                  <h5>add hot water</h5>
                </div>
                <div class="_icon _2">
                  <img src="https://cdn.shopify.com/s/files/1/0901/1216/4158/files/equal.svg?v=1736976069">
                </div>
                <div class="_item _3">
                  <img src="https://cdn.shopify.com/s/files/1/0901/1216/4158/files/jarra.svg?v=1736976069">
                  <h5>get cookin'</h5>
                </div>
              </div>
            </div>
          </div>
        </div>
        <figure class="_food zanahoria">
          <img src="https://cdn.shopify.com/s/files/1/0901/1216/4158/files/zanahoria.svg?v=1736975862" />
        </figure>
        <figure class="_food jengibre">
          <img src="https://cdn.shopify.com/s/files/1/0901/1216/4158/files/jengibre.svg?v=1736975862" />
        </figure>
      </div>
    </div>
  </section>
</main>


<script type="text/javascript">
  document.addEventListener('DOMContentLoaded', function () {
    let controller_home = new ScrollMagic.Controller({refreshInterval: 0});  
    let home_height_i = $("main>section._i").height();
    
    var anim_i_1 = TweenMax.to("main._home._1 section._i ._cardhome figure._food.pollo img", 0.5, {y: "-30px", ease: Power1.easeInOut});
    var scene_i_1 = new ScrollMagic.Scene({triggerElement: "#trigger_header", duration: (home_height_i) })
      .setTween(anim_i_1)
      .triggerHook(0.49)
      // .addIndicators()
      .addTo(controller_home);
    
    var anim_i_2 = TweenMax.to("main._home._1 section._i ._cardhome figure._food.cebolla img", 0.5, {y: "-100px", ease: Power1.easeInOut});
    var scene_i_2 = new ScrollMagic.Scene({triggerElement: "#trigger_header", duration: (home_height_i) })
      .setTween(anim_i_2)
      .triggerHook(0.49)
      // .addIndicators()
      .addTo(controller_home);
    
    var anim_i_3 = TweenMax.to("main._home._1 section._i ._cardhome figure._food.lemon img", 0.5, {y: "-50px", ease: Power1.easeInOut});
    var scene_i_3 = new ScrollMagic.Scene({triggerElement: "#trigger_header", duration: (home_height_i) })
      .setTween(anim_i_3)
      .triggerHook(0.49)
      // .addIndicators()
      .addTo(controller_home);
    
    var anim_i_4 = TweenMax.to("main._home._1 section._i ._cardhome figure._food.cilantro img", 0.5, {y: "-20px", ease: Power1.easeInOut});
    var scene_i_4 = new ScrollMagic.Scene({triggerElement: "#trigger_header", duration: (home_height_i) })
      .setTween(anim_i_4)
      .triggerHook(0.49)
      // .addIndicators()
      .addTo(controller_home);
    
    var anim_i_5 = TweenMax.to("main._home._1 section._i ._cardhome figure._food.jengibre img", 0.5, {y: "-80px", ease: Power1.easeInOut});
    var scene_i_5 = new ScrollMagic.Scene({triggerElement: "#trigger_header", duration: (home_height_i) })
      .setTween(anim_i_5)
      .triggerHook(0.49)
      // .addIndicators()
      .addTo(controller_home);
    
    var anim_i_6 = TweenMax.to("main._home._1 section._i ._cardhome figure._food.zanahoria img", 0.5, {y: "-40px", ease: Power1.easeInOut});
    var scene_i_6 = new ScrollMagic.Scene({triggerElement: "#trigger_header", duration: (home_height_i) })
      .setTween(anim_i_6)
      .triggerHook(0.49)
      // .addIndicators()
      .addTo(controller_home);
    
  });
</script>

<style>
  span#trigger_home_i-1 {
    top: 100%;
  }
</style>
